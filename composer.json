{
    "name": "imrealashu/2f-authentication",
    "description": "Two Factor Authentication (2FA) for Laravel",
    "license": "MIT",
    "keywords": [
        "laravel",
        "two-factor",
        "authentication",
        "2fa",
        "rfc-6238"
    ],
    "authors": [
        {
            "name": "Ashish Singh",
            "email": "imrealashu@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "Spomky-Labs/otphp": "^8.3",
        "laravel/framework": "5.4.*"
    },
    "autoload": {
        "psr-4": {
            "Thecodework\\TwoFactorAuthenticatioin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Thecodework\\TwoFactorAuthentication\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpunit/dbunit": "^2.0",
        "orchestra/testbench": "~3.4"
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
